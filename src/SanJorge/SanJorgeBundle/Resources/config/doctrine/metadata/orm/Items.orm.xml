<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Items" table="items">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idfactura" type="bigint" column="idfactura" nullable="true"/>
    <field name="producto" type="string" column="producto" length="200" nullable="true"/>
    <field name="cantidad" type="integer" column="cantidad" nullable="true"/>
    <field name="costoUnitario" type="float" column="costo_unitario" nullable="true"/>
    <field name="impuestos" type="float" column="impuestos" nullable="true"/>
    <field name="costoTotal" type="float" column="costo_total" nullable="true"/>
  </entity>
</doctrine-mapping>
